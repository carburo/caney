<body>
    <header id="header" class="navbar navbar-default affix-top" data-spy="affix" data-offset-top="400">
        <div class="container">
            <a alt="Caney" class="site-logo" href="{{ path('homepage') }}">
                <h2>Caney</h2>
            </a>
            <nav role="navigation" id="nav-main" class="okayNav">
                {{ knp_menu_render('AppBundle:Builder:mainMenu', {
                    'currentClass': 'active',
                    'lastClass': 'sub-menu',
                    'firstClass': 'sub-menu',
                    'branchClass': 'sub-menu'
                }) }}
            </nav>
        </div>
    </header>

{% block body %}
    {% block bodyPreamble %}{% endblock %}
    <div class="container" style="margin-top: 20px">
        <div class="page active">
            {% block heading %}
            <section id="title" xmlns="http://www.w3.org/1999/html">
                <div class="row">
                    <div class="col-sm-6">
                        <h1 class="header">{% block headingTitle %}{% endblock %}</h1>
                        <p>{% block headingSubtitle %}{% endblock %}</p>
                    </div>
                    {#{{ dump(knp_menu_get_breadcrumbs_array('AppBundle:Builder:mainMenu', 'menu.hostels')) }}#}

                    {% block breadcrumbsLayout %}
                    <div class="col-sm-6">
                        <ul class="breadcrumb pull-right">
                            {% block breadcrumbs %}
                            {% endblock %}
                        </ul>
                    </div>
                    {% endblock %}

                </div>
            </section>
            {% endblock %}

            {% block content %}
            {% endblock %}
        </div>
    </div>
{% endblock %}


